http://<%- site.themeId %>icongallery.netlify.com/* http://www.<%- site.themeId %>icongallery.com/:splat 301!
https://<%- site.themeId %>icongallery.netlify.com/* https://www.<%- site.themeId %>icongallery.com/:splat 301!

<%-
  site.icons.reduce((acc, icon) => {
    if (icon.redirectFrom) {
      // Might be a string (single redirect) or array (multiple redirects)
      // So we'll account for that.
      const redirects = [].concat(icon.redirectFrom);
      return [
        ...acc,
        ...redirects.map(redirect => `${redirect} ${icon.permalink}`)
      ]
    } else {
      return acc;
    }
  }, []).join("\n")
%>
    