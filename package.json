{
  "name": "icon-galleries",
  "private": true,
  "scripts": {
    "copy-content": "sync-files ../_posts ./src/www/icons",
    "start": "DEBUG=metalsmith-timer,metalsmith-build-info node metalsmith.js",
    "create-post": "node scripts/create-post.js",
    "build": "NODE_ENV=production DEBUG=metalsmith-timer,metalsmith-build-info,metalsmith node metalsmith.js",
    "build-netlify": "npm run copy-content && npm run build",
    "react-start": "webpack --mode=development --config webpack.config.js --output-path ./build/assets/scripts --watch",
    "react-build": "webpack --mode=production --config webpack.config.js",
    "watchos-get-icon": "babel-node save-watch-os-512-img.js --presets es2015,stage-2"
  },
  "dependencies": {
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "string_score": "0.1.22"
  },
  "devDependencies": {
    "ejs": "^2.6.1",
    "jstransformer-ejs": "^0.2.0",
    "metalsmith": "^2.1.0",
    "metalsmith-debug": "^1.2.0",
    "metalsmith-debug-ui": "^0.3.0",
    "metalsmith-ignore": "^0.1.2",
    "metalsmith-in-place": "^4.1.1",
    "metalsmith-layouts": "^1.4.1",
    "metalsmith-pagination": "^1.4.0",
    "metalsmith-rename": "^1.0.0",
    "metalsmith-serve": "0.0.7",
    "metalsmith-timer": "0.0.2",
    "metalsmith-watch": "^1.0.3",
    "minimist": "^1.2.0",
    "moment": "^2.22.2",
    "slug": "^0.9.1",
    "sync-files": "^1.0.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "node-fetch": "^2.2.0",
    "prompt": "^1.0.0",
    "readline-sync": "^1.4.9",
    "request": "2.85.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  }
}
